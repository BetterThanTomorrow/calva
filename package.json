            {
                "command": "calva.toggleCLJCSession",
                "title": "Toggle the REPL connection (clj or cljs) used for CLJC files",
                "category": "Calva"
            },
            {
                "command": "calva.selectCurrentForm",
                "title": "Select current form",
                "category": "Calva"
