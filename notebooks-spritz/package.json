{
  "name": "calva-spritz",
  "displayName": "Calva Spritz",
  "publisher": "betterthantomorrow",
  "description": "A LiveShare + Calva enabling workaround, effervescently crafted as A bubbly association from Clojure files to Notebook.",
  "version": "1.0.5",
  "icon": "spritz-icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/BetterThanTomorrow/calva.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BetterThanTomorrow/calva/issues"
  },
  "engines": {
    "vscode": "^1.67.0"
  },
  "sponsor": {
    "url": "https://calva.io/sponsors/"
  },
  "categories": [
    "Notebooks"
  ],
  "keywords": [
    "Notebooks",
    "Clojure",
    "Calva"
  ],
  "contributes": {
    "notebooks": [
      {
        "type": "calva-clojure-notebook",
        "displayName": "Clojure Notebook",
        "priority": "option",
        "selector": [
          {
            "filenamePattern": "*.{clj,cljc,cljs,cljd,cljx,clojure,joke,bb,lingy,calva-repl,ly,lpy,fiddle}"
          }
        ]
      }
    ]
  }
}
