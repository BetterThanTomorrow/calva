{
    "compilerOptions": {
        "moduleResolution": "node",
        "target": "es5",
        "lib": [
            "es2016",
            "dom"
        ],
        "jsx": "react",
        "downlevelIteration": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "declaration": false,
        "noImplicitAny": false,
        "noImplicitUseStrict": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true
    },
    "compileOnSave": false,
    "buildOnSave": false,
}